{
  "expo": {
    "name": "Selene",
    "slug": "selene",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "selene",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.selene.marketplace",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "selene",
              "host": "auth-verified"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": true,
      "package": "com.selene.marketplace"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosClientId": "332992495631-gpl4f50ndu6oji4btgtkpohtukuvj622.apps.googleusercontent.com",
          "webClientId": "332992495631-gnackhq8269n8v4k7e12sefsfs0cj5ep.apps.googleusercontent.com",
          "androidClientId": "332992495631-tnk4upn2o4e5k2026c5ee8f21ijmi120.apps.googleusercontent.com",
          "iosUrlScheme": "com.googleusercontent.apps.332992495631-gpl4f50ndu6oji4btgtkpohtukuvj622"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Selene necesita acceso a tus fotos para que puedas subir tu avatar y publicar productos.",
          "cameraPermission": "Selene necesita acceso a tu c√°mara para que puedas tomar fotos de tus productos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "48bbf920-2548-4855-a45e-4031d7a50bc7"
      }
    },
    "owner": "alvaroest19"
  }
}
